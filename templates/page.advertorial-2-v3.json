        "heading_Pgwxz9": {
          "type": "heading",
          "settings": {
            "text": "Schützen Sie Ihre Füße im Frühjahr mit diesem revolutionären und wasserabweisendem Schuh!",
            "heading_tag": "h3",
            "text_color": "",
            "gradient": ""
          }
