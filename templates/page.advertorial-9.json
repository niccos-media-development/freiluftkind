        "heading_Pgwxz9": {
          "type": "heading",
          "settings": {
            "text": "Orthopäde deckt auf: Warum Barfußschuhe mehr als ein Trend sind – Ihre Gesundheit könnte davon abhängen!",
            "heading_tag": "h3",
            "text_color": "",
            "gradient": ""
          }
        },
        "author_6PYk8k": {
          "type": "author",
          "settings": {
            "imageadvheadimg": "shopify://shop_images/freiluftkind_lothar_krueger.jpg",
            "imageadvheadtext": "<p>vom <strong>Orthopäden Lothar Krüger</strong></p>"
          }
        },
        "image_AF7UjR": {
          "type": "image",
          "settings": {
            "image": "shopify://shop_images/lothar_adv.jpg",
            "width_mode": "full_width",
            "max_width": 1000,
